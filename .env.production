# Production Environment Configuration
NODE_ENV=production

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# App Configuration
VITE_APP_URL=https://joinomu.com
VITE_APP_NAME=JoinOmu
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_TIMEOUT=30000
VITE_MAX_RETRIES=3

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_HEALTHKIT=true
VITE_ENABLE_NOTIFICATIONS=true

# Security
VITE_CONTENT_SECURITY_POLICY=true
VITE_STRICT_TRANSPORT_SECURITY=true

# Monitoring
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
OTEL_EXPORTER_OTLP_ENDPOINT=https://api.honeycomb.io
OTEL_EXPORTER_OTLP_HEADERS=x-honeycomb-team=your-api-key

# Performance
VITE_ENABLE_SW=true
VITE_CACHE_MAX_AGE=86400

# Healthcare Compliance
VITE_HIPAA_COMPLIANCE=true
VITE_AUDIT_LOGGING=true
VITE_DATA_RETENTION_DAYS=2555  # 7 years for healthcare data