{
  "name": "@joinomu-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "storybook": "storybook dev -p 6006 --config-dir .storybook",
    "build-storybook": "storybook build --config-dir .storybook",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "build": "npm run build:shared && npm run build:ui && npm run build:web",
    "build:shared": "cd shared && npm run build",
    "build:ui": "cd ui && npm run build",
    "build:web": "cd apps/web && npm run build",
    "dev:web": "cd apps/web && npm run dev",
    "dev:mobile": "cd apps/mobile && npm start",
    "clean": "rm -rf node_modules apps/*/node_modules ui/node_modules shared/node_modules",
    "fresh-install": "npm run clean && npm install"
  },
  "private": true,
  "dependencies": {
    "@expo/metro-config": "~0.20.14",
    "@expo/metro-runtime": "~5.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
