  },
  "workspaces": [
    "apps/*",
    "shared",
    "config",
    "ui",
    "mobile-ui",
    "mobile"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
