    "@nx/playwright": "21.5.2",
    "@nx/react": "21.5.2",
    "@nx/rollup": "21.5.2",
    "@nx/vite": "21.5.2",
    "@nx/web": "21.5.2",
    "@nx/workspace": "21.5.2",
    "@playwright/test": "^1.36.0",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
